{
  "expo": {
    "name": "LAZ Al Azhar 5",
    "slug": "baitulmaal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios_appstore_icon_1024.png",
    "scheme": "baitulmaal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/ios_appstore_icon_1024.png",
      "bundleIdentifier": "com.mekayastudio.baitulmaal",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/images/playstore_icon_1024.png",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/playstore_icon_1024.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "blockedPermissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ],
      "package": "com.mekayastudio.baitulmaal",
      "versionCode": 14
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ios_appstore_icon_1024.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ios_appstore_icon_1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/adzan.wav"
          ]
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Izinkan LAZ Al Azhar 5 mengakses lokasi Anda untuk menampilkan jadwal sholat sesuai wilayah."
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": false,
          "microphonePermission": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Izinkan LAZ Al Azhar 5 mengakses foto Anda.",
          "savePhotosPermission": "Izinkan LAZ Al Azhar 5 menyimpan QRIS ke galeri Anda.",
          "granularPermissions": [
            "photo"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6a5fadf2-9933-4026-9186-09bf3c4cc257"
      },
      "privacyPolicyUrl": "https://laz.rizqis.com/privacy-policy",
      "termsOfServiceUrl": "https://laz.rizqis.com/terms-of-service",
      "supportUrl": "https://laz.rizqis.com/support"
    }
  }
}
